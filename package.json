{
  "name": "budjet",
  "version": "0.0.1",
  "description": "A Simple Bill Tracker",
  "productName": "BudJet",
  "author": "Deven Phillips <deven.phillips@redhat.com>",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "watch": "npm-watch",
    "prism": "prism mock -d --cors -p 7080 openapi.yml",
    "openapi": "openapi-generator-cli generate -g typescript-axios -i openapi.yml -o src/sdk/ -p withSeparateModelsAndApi=true,apiPackage=api,modelPackage=models,legacyDiscriminatorBehavior=false"
  },
  "watch": {
    "openapi": {
      "patterns": [
        "openapi.yml"
      ]
    },
    "prism": true,
    "dev": {
      "inherit": true,
      "patterns": [
        "package.json",
        "quasar.conf.js",
        "yarn.lock"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.24.0",
    "highcharts": "^9.3.2",
    "highcharts-vue": "^1.4.0",
    "core-js": "^3.6.5",
    "@quasar/extras": "^1.12.3",
    "pinia": "^2.0.9",
    "quasar": "^2.4.10",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.25",
    "@quasar/vite-plugin": "^1.0.4",
    "@vitejs/plugin-vue": "^2.0.0",
    "@stoplight/prism-cli": "^4.6.1",
    "npm-watch": "^0.11.0",
    "sass": "1.32.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
